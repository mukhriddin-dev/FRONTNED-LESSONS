{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*":["./src"],
      "@components/*":["src/components/*"],
      "@ui/*":["src/components/ui/*"],
      "@assets/*": ["src/assets/*"],
      "@layouts/*":["src/layouts/*"],
      "@type/*":["src/types/*"],
      "@mocks/*":["src/mocks/*"]
    }
  }
}